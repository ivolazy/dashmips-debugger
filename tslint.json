{
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "curly": true,
        "class-name": true,
        "semicolon": [
            true,
            "never"
        ],
        "triple-equals": true,
        "max-line-length": [
            true,
            180
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "object-literal-sort-keys": false
    },
    "defaultSeverity": "warning"
}
