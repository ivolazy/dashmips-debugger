{
	"rules": {
		"quotemark": [true, "single"],
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true,
		"max-line-length": [true, 80]
	},
	"defaultSeverity": "warning"
}
